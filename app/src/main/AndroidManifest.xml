<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.smtgroup.estateapplication">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SignIN" />
        <activity android:name=".SignIn" />
        <activity android:name=".SignUp" />
        <activity
            android:name=".HomepageType"
            android:label="@string/title_activity_homepage"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".HomepageCategory"
            android:label="@string/title_activity_homepage_category"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".HomepageHouseCategory"
            android:label="@string/title_activity_homepage_house_category"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".AdPage"
            android:label="@string/title_activity_ad_page"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".HomepageBusinessCategory"
            android:label="@string/title_activity_homepage_business_category"
            android:theme="@style/AppTheme.NoActionBar" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />

        <activity android:name=".Detail"></activity>
    </application>

</manifest>